{
  "name": "pokemon-finder-react",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "apisauce": "^1.1.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "i18next": "^13.1.5",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.4.1",
    "react-spinkit": "^3.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "wolox-equalizer": "0.0.3"
  },
  "scripts": {
    "start": "node ./scripts/start.js development",
    "start-env": "node ./scripts/start.js",
    "build": "node ./scripts/build.js",
    "test": "rescripts test --env=jsdom",
    "eject": "./node_modules/react-scripts/bin/react-scripts.js eject",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "lint-fix": "./node_modules/eslint/bin/eslint.js src --fix && ./node_modules/stylelint/bin/stylelint.js '**/*.scss' --fix",
    "lint-scss": "./node_modules/stylelint/bin/stylelint.js '**/*.scss'",
    "lint-diff": "git diff --name-only --cached --relative --diff-filter=ACM | grep \\.js$ | xargs eslint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@rescripts/cli": "0.0.7",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.30",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack-env": "^1.15.1",
    "env-cmd": "^9.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-wolox": "^3.0.2",
    "eslint-config-wolox-react": "^2.0.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^2.7.0",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2",
    "prop-types": "^15.7.2",
    "react-docgen": "^5.3.0",
    "react-hot-loader": "^4.12.20",
    "stylelint": "^13.2.1",
    "stylelint-config-wolox": "^1.0.5",
    "stylelint-no-indistinguishable-colors": "^1.2.1",
    "stylelint-scss": "^3.5.4"
  },
  "title": "pokemon-finder-react",
  "repository": {
    "type": "git",
    "url": "git@github.com:raparicio6/pokemon-finder-react.git"
  },
  "engines": {
    "node": ">= 10.15.3",
    "npm": ">= 6.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-diff && npm run lint-scss"
    }
  }
}
